<h1>Spotify Demo</h1>

<%= form_tag artists_path, method: "get" do %>
	<div class="form-group">
		<%= text_field_tag( :artist_name, "", placeholder: "Enter artist name", class: "form-control" ) %>
	</div>
	<%= submit_tag "Search" %>
<% end %>

